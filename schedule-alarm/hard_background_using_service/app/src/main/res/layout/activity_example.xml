<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".example.ExampleActivity"
    >

    <TextView
        android:id="@+id/actExampleTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="#000000"
        android:textSize="40sp"
        android:textStyle="bold"
        />

    <Button
        android:id="@+id/actExampleGetString"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="getString"
        />

    <Button
        android:id="@+id/actExampleGetInt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="getInt"
        />

    <Button
        android:id="@+id/actExampleGetStringAsync"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="getStringAsync"
        />

    <Button
        android:id="@+id/actExampleGetIntAsync"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="getIntAsync"
        />


</LinearLayout>